<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.lobbyview.LobbyVC">
   <children>
      <VBox prefHeight="255.0" prefWidth="600.0">
         <children>
            <Label fx:id="userTypeLabel" text="User type: ">
               <VBox.margin>
                  <Insets left="30.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="pinLabel" text="Pin:">
               <font>
                  <Font size="54.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin></Label>
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#StartQuizButton" prefHeight="25.0" prefWidth="72.0" text="Start quiz">
               <VBox.margin>
                  <Insets left="280.0" top="40.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
